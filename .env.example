# Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# URL de tu API de búsqueda semántica desplegada en Hugging Face
NEXT_PUBLIC_API_URL=https://your-huggingface-space.hf.space

# Token de autorización de Hugging Face
HUGGING_FACE_TOKEN=your_hugging_face_token_here

# Token de Mapbox (opcional, mejora el rendimiento del mapa)
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Gemini API Key for Chat
GEMINI_API_KEY=your_gemini_api_key_here
